import re

# . (dot ) matches any character except new line 
match = re.search('n..n','Its a cool world anshulanshul')
if match:
  print(match.group())
# /w word char
# /d digit
# /s white space
match= re.search('ansi', 'Its a cool worldanshul')
if match:
  print(match.group())
else:
  print('Not Found')  